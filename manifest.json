{
  "manifest_version": 2,
  "name": "Sassy tab",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "chrome_url_overrides": { "newtab": "index.html" },
  "icons": { "16": "media/icon.png",
    "48": "media/icon.png",
   "128": "media/icon.png" },
   "description": "A very sassy new tab plugin",
   "author": "Nikki Sollid"
}